<odoo>
    <data noupdate="1">
        <record id="seq_hr_expense_ati" model="ir.sequence">
            <field name="name">HR Expense ATI</field>
            <field name="code">hr.expense.ati</field>
            <field name="prefix">ATI-</field>
            <field name="padding">3</field>
            <field name="implementation">no_gap</field>
        </record>
        <record id="seq_hr_expense_cti" model="ir.sequence">
            <field name="name">HR Expense CTI</field>
            <field name="code">hr.expense.cti</field>
            <field name="prefix">CTI-</field>
            <field name="padding">3</field>
            <field name="implementation">no_gap</field>
        </record>

        <record id="seq_hr_expense" model="ir.sequence">
            <field name="name">HR Expense Sequence</field>
            <field name="code">hr.expense</field>
            <field name="prefix">EXP</field>
            <field name="padding">4</field>  <!-- 0001, 0002, etc. -->
            <field name="number_increment">1</field>
        </record>
        <!-- Add more sequences for other companies as needed -->
    </data>
</odoo>
